{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "bouquet-of-roses/split-feature",
	"version": "0.1.0",
	"title": "Split Feature",
	"category": "layout",
	"icon": "columns",
	"description": "Block with media on one column and text in the other.",
	"supports": {
		"html": false
	},
	"attributes": {
		"title": {
			"type": "string"
		},
		"paragraph": {
			"type": "string"
		},
		"mediaAlignment": {
			"type": "boolean"
		},
		"media": {
			"type": "object",
			"default": {
				"id": 0,
				"url": "",
				"height": 0,
				"width": 0,
				"alt": ""
			}
		}
	},
	"textdomain": "split-feature",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}